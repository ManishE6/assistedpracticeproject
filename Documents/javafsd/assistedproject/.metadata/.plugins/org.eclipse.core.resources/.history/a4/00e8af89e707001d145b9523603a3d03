//implicit
public class typecasting {
	public static void main(String[] args) {
		int i = 10;
        long l = i;
        float f = l;
        System.out.println("Int value " + i);
        System.out.println("Long value " + l);
        System.out.println("Float value " + f);
    }

	
	
//explicit

long l = 100;
int i = (int)l;
System.out.println("Long value " + l);
System.out.println("Int value " + i);
}



